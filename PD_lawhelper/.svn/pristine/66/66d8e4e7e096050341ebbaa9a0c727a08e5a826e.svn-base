        {
        	this.ds_search.clearData();
        	var rowIdx = this.ds_search.addRow();
        	
        	
        	trace("컨디션 : "+this.div_search.cmb_searchCondition.value);	
        	trace("키워드 : "+this.div_search.edit_searchKeyword.value); 
        	this.div_search.cmb_searchCondition.value
        	
