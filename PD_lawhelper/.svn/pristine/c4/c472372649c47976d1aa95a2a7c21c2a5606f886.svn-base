        	var lawsuitAdmNo = this.ds_lawsuit.getColumn(0, "lawsuitAdmNo");
        	var instAdmCode = this.ds_lawsuit.getColumn(0, "instAdmCode");
        	this.ds_preiwr.insertRow(0);
