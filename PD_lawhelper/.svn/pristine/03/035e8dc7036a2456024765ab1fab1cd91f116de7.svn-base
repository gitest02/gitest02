this.btn_mediation_delete_OnClick = function(obj:Button, e:nexacro.ClickEventInfo)
{

	var row = this.ds_mediationDetail.rowposition;
	if(this.ds_mediationDetail.rowcount == 0){
		alert("삭제할 데이터가 없습니다.");
		return;
    }
	this.ds_mediationDetail.deleteRow(row);
	
	alert("삭제 되었습니다.");
	
}



