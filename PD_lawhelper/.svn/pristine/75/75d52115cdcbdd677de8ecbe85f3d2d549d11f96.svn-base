			nDataType : 0, // (0: XML타입)
			bCompress : false,			
		};
				
		Iject.transaction(this,oDate,function(){
			if(this.errorcode < 0)
			{
				Iject.alert("삭제되었습니다.")
				
			}
	
		});
	}else{
