 *******************************************************************************/
function form_OnLoadCompleted(obj){

	comboSetup(); 	// 공통코드 콤보데이터에 맞게 분할
	chnScrn = ds_ComInfo.getColumn(0,"searchValue");  //화면구분값 확인	
	
	if(chnScrn == "1"){   //퀵등록메뉴에서 화면 이동 되었을 경우
		lawsrcrlt = ds_ComInfo.getColumn(0,"lawsuitAdmNo"); 	//소송관리번호	
		instcdrlt = ds_ComInfo.getColumn(0,"instAdmCode");	//심급코드	
		getLawsuitDetail(lawsrcrlt, instcdrlt);	 //조정안 기본사항 검색
		ds_pladef.rowposition = -1;
		btn_med_approve.enable=false;
		Button10.enable=false;
			
	}else if(chnScrn == "2"){	//결재 및 상신리스트화면 이동 되었을 경우
		var aprNo = ds_ComInfo.getColumn(0,"aprvprgrInfoBookNo");   //결재대장번호

	    if( aprNo != null ){
			lawsrcrlt = SubStr(aprNo, 0 ,10 );	//소송관리번호
			instcdrlt = SubStr(aprNo, 13 ,3 );  //심급코드			
			
			ds_ComInfo.setColumn(0,"lawsuitAdmNo",lawsrcrlt);
			ds_ComInfo.setColumn(0,"instAdmCode",instcdrlt);
			
			pSeq = ParseInt( SubStr(aprNo, 16 , 4 ) ) ;
			
			getLawsuitDetail(lawsrcrlt, instcdrlt);	 //조정안 기본사항 검색
			//ds_pladef.row = ds_pladef.FindRow( "pladefSeq", pSeq );	// 원피고일련번호
			ds_pladef.rowposition = -1;
			btn_med_approve.enable=false;
			Button10.enable=false;
		}	
		
	}else if(chnScrn == "3"){	//조정안 & 재판외화해 화면 이동 되었을 경우
	
		lawsrcrlt = Parent.ds_ComInfo.getColumn(0,"lawsuitAdmNo");	//소송관리번호
		instcdrlt = Parent.ds_ComInfo.getColumn(0,"instAdmCode");	//심급코드
		pSeq = Parent.ds_ComInfo.getColumn(0,"pladefSeq");			//원피고일련번호
		pSepcode = Parent.ds_ComInfo.getColumn(0,"pladefLawConcSepcode");
		
		getLawsuitDetail(lawsrcrlt, instcdrlt);	 //조정안 기본사항 검색
		
		ds_pladef.row = ds_pladef.FindRow( "pladefSeq", pSeq );	// 원피고일련번호	
		ds_pladef.rowposition = pSeq;
	} 
}

