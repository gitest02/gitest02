this.div_top_btn_group_insert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var today = new Date();
	var dd = today.getDate();
	var m = today.getMonth()+1;
	var yyyy = today.getFullYear();
	var mstr = new String();
	mstr = m;
	if(m<10){
	 var mm = "0"+mstr
	}else{
	 var mm = m;
	}
	
	today = yyyy+"-"+mm+"-"+dd;

	var nRow = this.ds_codemaster.addRow();
	//새로운 행 추가시 시간 넣기
	this.ds_codemaster.setColumn(nRow,"regist_time",today);
	//새로운 행 추가시 사용자명 넣기
	this.ds_codemaster.setColumn(nRow,"regist_id",application.gds_userInfo.getColumn(0,"userid"));

}

