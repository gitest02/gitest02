				Iject.alert("저장완료");
				break;
	      } 
	       var pThis = this.fobj;   //form object	      
	      
	});
	
    this.ds_schedulelist.addRow();
    this.ds_schedulelist.setColumn(0,"userid",application.gds_userInfo.getColumn(0,14));
	this.reload();
}

