{
	if(obj.value.toString().substr(0, 4) == "0000") {
		var currDate = new Date();
		var year = currDate.getFullYear().toString().padLeft(4, "0");
		var month = (currDate.getMonth()+1).toString().padLeft(2, "0");
		var day = currDate.getDate().toString().padLeft(2, "0");
		currDate = year+month+day;
		obj.set_value(currDate);	
	} else {
	
	}
	var endDate = obj.value / 100000;
	var row = this.ds_fixinfo.rowposition;
	this.ds_fixinfo.setColumn(row, "lawFixinfoDlinDate", endDate);
}

