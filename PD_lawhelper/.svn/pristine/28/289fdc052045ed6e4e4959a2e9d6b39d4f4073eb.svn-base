this.autoHypenbirth = function(str){
var tmp = '';
  if(str.length < 1){
    return str;
  }else if(str.length < 4){
	tmp += str.substr(0, 2);
    tmp += '-';
    tmp += str.substr(2);
    this.ed_birth.value.trim();
    return tmp;
  }
 
  else if(str.length < 6){
    tmp += str.substr(0, 2);
    tmp += '-';
    tmp += str.substr(2, 2);
    tmp += '-';
    tmp += str.substr(4);
    this.ed_birth.value.trim();
    return tmp;
  }else{        
    tmp += str.substr(0, 2);
    tmp += '-';
    tmp += str.substr(2, 2);
    tmp += '-';
    tmp += str.substr(4);
    this.ed_birth01.value.trim();
    this.ed_birth01.set_value(tmp);
    return tmp;
  }
  }

