this.V_SCD_003_onmouseenter = function(obj:Form, e:nexacro.MouseEventInfo)
{
	var tdate = this.ed_date.value;
	var rdate = this.ds_result.getColumn(0,"eventdate");
	if(rdate==null){
	return true;
	}else{
	var sdate = rdate.substring(0,10);
	if(tdate!=sdate){
	this.reload();
	return true;
	}
	}
