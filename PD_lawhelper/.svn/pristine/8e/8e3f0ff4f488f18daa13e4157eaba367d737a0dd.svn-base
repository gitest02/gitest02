        this.ed_phone_onkeyup = function(obj,e)
        {
        	var str = this.ed_phone.value;
        	this.ed_phone.set_maxlength(13);
        	this.autoHypenPhone(str);
        }

        this.autoHypenPhone = function(str){
        var tmp = '';
          if(str.length < 4){
            return str;
          }else if(str.length < 7){
        	tmp += str.substr(0, 3);
            tmp += '-';
            tmp += str.substr(3);
            this.ed_phone.value.trim();
            return tmp;
          }else if(str.length < 11){
            tmp += str.substr(0, 3);
            tmp += '-';
            tmp += str.substr(3, 3);
            tmp += '-';
            tmp += str.substr(6);
            this.ed_phone.value.trim();
            return tmp;
          }else{        
            tmp += str.substr(0, 3);
            tmp += '-';
            tmp += str.substr(3, 4);
            tmp += '-';
            tmp += str.substr(7);
            this.ed_phone.value.trim();
            this.ed_phone.set_value(tmp);
            return tmp;
          }
          }
        this.ed_birth_onkeyup = function(obj,e)
        {
        	var str = this.ed_birth.value;
        	this.ed_birth.set_maxlength(6);
        	this.autoHypenbirth(str);
        }

        this.autoHypenbirth = function(str){
        var tmp = '';
          if(str.length < 1){
            return str;
          }else if(str.length < 4){
        	tmp += str.substr(0, 2);
            tmp += '-';
            tmp += str.substr(2);
            this.ed_birth.value.trim();
            return tmp;
          }
         
          else if(str.length < 6){
            tmp += str.substr(0, 2);
            tmp += '-';
            tmp += str.substr(2, 2);
            tmp += '-';
            tmp += str.substr(4);
            this.ed_birth.value.trim();
            return tmp;
          }else{        
            tmp += str.substr(0, 2);
            tmp += '-';
            tmp += str.substr(2, 2);
            tmp += '-';
            tmp += str.substr(4);
            this.ed_birth.value.trim();
            this.ed_birth.set_value(tmp);
            return tmp;
          }
          }
          
