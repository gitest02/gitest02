this.fn_goPage = function(page)
{
	var oObj = {
          ds    : this.ds_menu,    // 메뉴 dataset 
          nRow  : page,              // 선택된 
          oArgs : []               // 넘길 argument
    };
    Iject.Mdi.call(this,oObj);
    
}

