function aprv_lock(){
	var id=SubStr(gb_Param,"4","10");
	
    if(ds_position.GetColumn(0, "aprvInfoCont") == null){
		btn_med_approve.enable=true;
		Button10.enable=true;
		enable_true();

	}else if(ds_position.GetColumn(0, "aprvInfoCont") == ""){
		if(ds_position.GetColumn(0, "aprvprgrInfoWritSabun") == id){
			btn_med_approve.enable=true;
			Button10.enable=true;
			enable_true();	
			
		}else if(ds_position.GetColumn(0, "aprvInfoSabun") == id){
			btn_med_approve.enable=true;
			Button10.enable=false;
			enable_false();
			
		}else {
		    btn_med_approve.enable=false;
			Button10.enable=false;
			enable_false();
		}
	}else if(ds_position.GetColumn(1, "aprvInfoCont") == ""){
		if(ds_position.GetColumn(0, "aprvInfoSabun") == id){
			btn_med_approve.enable=true;
			Button10.enable=false;
			enable_false();
			
		}else if(ds_position.GetColumn(1, "aprvInfoSabun") == id){
			btn_med_approve.enable=true;
			Button10.enable=false;
			enable_false();
			
		}else{
			btn_med_approve.enable=false;
			Button10.enable=false;
			enable_false();
		}
	}else if(ds_position.GetColumn(2, "aprvInfoCont") == ""){
		if(ds_position.GetColumn(1, "aprvInfoSabun") == id){
			btn_med_approve.enable=true;
			Button10.enable=false;
			enable_false();
			
		}else if(ds_position.GetColumn(2, "aprvInfoSabun") == id){
			btn_med_approve.enable=true;
			Button10.enable=false;
			enable_false();
			
		}else{
			btn_med_approve.enable=false;
			Button10.enable=false;
			enable_false();
		}
	}else if(ds_position.GetColumn(2, "aprvInfoCont") != ""){
		if(ds_position.GetColumn(2, "aprvInfoSabun") == id){
			btn_med_approve.enable=true;
			Button10.enable=false;
			enable_false();
			
		}else{
			btn_med_approve.enable=false;
			Button10.enable=false;
			enable_false();
		}
	}
}
