        // User Script
        this.addIncludeScript("V_REC_002.xfdl", "Lib::Comm.xjs");
        this.registerScript("V_REC_002.xfdl", function() {
        ////////////////////////////////////////////////////////////////////////////////
        /*
        작 성 자 : 이창민
