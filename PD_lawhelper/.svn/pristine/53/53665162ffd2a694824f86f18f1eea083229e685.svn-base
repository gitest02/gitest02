this.Div06_Button03_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{

 	var lawFixinfoPlc = this.Div06.Div09.Edit00.value;
 	var lawFixinfoSepName = this.Div06.Div11.ListBox00.text;
 	var lawFixinfoSepCode = this.Div06.Div11.ListBox00.value;

	if(lawFixinfoPlc == null) {
		alert("기일장소를 확인해주세요");
		return;
	}
	
	if(lawFixinfoSepCode == null) {
		alert("기일구분을 확인해주세요");
		return;
	}

	var currDate = new Date();
	var year = currDate.getFullYear().toString().padLeft(4, "0");
	var month = (currDate.getMonth()+1).toString().padLeft(2, "0");
	var day = currDate.getDate().toString().padLeft(2, "0");
	var lawFixinfoDate = this.Div06.Div02.Calendar00.value;
	if(lawFixinfoDate == null) {
		lawFixinfoDate = year+month+day;
	}
	
	var time = this.Div06.Div02.Combo00.value;
	var hour = new Number(this.Div06.Div02.Edit01.value);
	var min = new Number(this.Div06.Div02.Edit02.value);	
	if(time == "1") {
		if(isNaN(hour) || hour < 0 || hour > 12) {
			alert("시간을 확인해주세요");
			return;
		}
		if(hour == 12) {
			hour = hour - 12;
		}
		hour = hour.toString().padLeft(2, "0");
	} else {
		if(isNaN(hour) || hour < 0 || hour > 12) {
			alert("시간을 확인해주세요");
			return;
		}
		if(hour != 12) {
			hour = hour + 12;
		}
		hour = hour.toString().padLeft(2, "0");
	}
	
	if(isNaN(min) || min < 0 || min >= 60) {
		alert("분을 확인해주세요");
		return;
	}
	min = min.toString().padLeft(2, "0");
	
	lawFixinfoDate = lawFixinfoDate + hour + min;

	this.ds_fixinfo.insertRow(0);
	this.ds_fixinfo.setColumn(0, "lawsuitAdmNo", this.ds_search.getColumn(0, "lawsuitAdmNo"));
	this.ds_fixinfo.setColumn(0, "instAdmCode", this.ds_search.getColumn(0, "instAdmCode"));
	this.ds_fixinfo.setColumn(0, "lawFixinfoSepCode", lawFixinfoSepCode);	
	this.ds_fixinfo.setColumn(0, "lawFixinfoSepName", lawFixinfoSepName);	
	this.ds_fixinfo.setColumn(0, "lawFixinfoPlc", lawFixinfoPlc);
	this.ds_fixinfo.setColumn(0, "lawFixinfoDate", lawFixinfoDate);	
	
	this.Div03.set_visible(true);
	this.Div06.set_visible(false);
	this.Button03.set_visible(true);
	this.Button00.set_visible(true);
	
	this.Div06.Div09.Edit00.set_value(null);
	this.Div06.Div11.ListBox00.set_index(-1);
	this.Div06.Div02.Calendar00.set_value(null);
	this.Div06.Div02.Combo00.set_index(0);
	this.Div06.Div02.Edit01.set_value(null);
	this.Div06.Div02.Edit02.set_value(null);	
	
}

