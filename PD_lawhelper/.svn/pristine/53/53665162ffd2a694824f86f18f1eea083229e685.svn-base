this.lawFixinfoChange = function() {
	var rowCnt = this.ds_fixinfo.rowcount;
	
	if(rowCnt == 0){
		if(deleteBtnClick) {
			deleteBtnClick = false;
		} else {
			alert("저장할 내용이 없습니다.");
			return;
		}
	} else {
		var rowCnt = this.ds_fixinfo.rowcount;
		for(i = 0; i<rowCnt; i++) {
			var lawFixinfoOthInsi = this.ds_fixinfo.getColumn(i, "lawFixinfoOthInsi");
			var lawFixinfoCortOpin = this.ds_fixinfo.getColumn(i, "lawFixinfoCortOpin");
			var lawFixinfoMainIssu = this.ds_fixinfo.getColumn(i, "lawFixinfoMainIssu");
			var lawFixinfoCpInsi = this.ds_fixinfo.getColumn(i, "lawFixinfoCpInsi");
			var lawFixinfoLyrOpin = this.ds_fixinfo.getColumn(i, "lawFixinfoLyrOpin");
			var lawFixinfoDlinDate = this.ds_fixinfo.getColumn(i, "lawFixinfoDlinDate");
		
			if(lawFixinfoDlinDate == null){
				alert("마감일시를 입력해주세요");
				return;
			}	
			if(lawFixinfoOthInsi == null || lawFixinfoOthInsi == ""){
				alert("상대방주장을 입력해주세요");
				return;
			}
			if(lawFixinfoCortOpin == null || lawFixinfoCortOpin == ""){
				alert("재판부의견을 입력해주세요");
				return;
			}
			if(lawFixinfoMainIssu == null || lawFixinfoMainIssu == ""){
				alert("주요쟁점을 입력해주세요");
				return;
			}
			if(lawFixinfoCpInsi == null || lawFixinfoCpInsi == ""){
				alert("당사주장을 입력해주세요");
				return;
			}
			if(lawFixinfoLyrOpin == null || lawFixinfoLyrOpin == ""){
				alert("변호사의견을 입력해주세요");
				return;
			}
		}
	}
	
	this.lawFixinfoSave();
	
	alert("저장되었습니다");	

}

