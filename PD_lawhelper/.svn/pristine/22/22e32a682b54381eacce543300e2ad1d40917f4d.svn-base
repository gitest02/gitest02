        this.fn_listClick = function(obj,e)
         {
         	//Grid내의 선택한 row의 위치 가져오기 
        	var nToRow = 0;
        	var nFromRow = e.row;	//복사할 row의 Position, int값
        	//선택한 row위치의 값들을 검색용 ds에 복사하기
