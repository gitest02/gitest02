    <Script type="xscript5.0"><![CDATA[

function Combo00_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	var code=Combo00.text;
	if(code=="전체"){
		search_form();
	}else{
		if(code=="파일이름"){
			code="001";	
		}else if(code=="파일확장자"){
			code="002";	
		}else{
			code="003";	
		}
		search_code(code);
	}
}]]></Script>
