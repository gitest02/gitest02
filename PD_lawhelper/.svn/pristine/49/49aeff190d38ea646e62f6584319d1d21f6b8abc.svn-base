    <Script type="xscript5.0"><![CDATA[/********************************************************************************
 * Name			: comp_lawsuit
 * Title		: 소송종결
 * Desc			: 소송종결 조회 및 저장,수정
 * 작성자		: 
 * 작성일		: 2013.07 ~ 2013.08
 * 변경사항	: 2014.01 ~ 2014.02 aprv_search고도화 작업  
 *******************************************************************************/
 
#include "lib::common.js";

var lawsrcrlt = "";		//소송관리번호
var	instcdrlt = "";		//심급코드
var pSeq = "";			//원피고 일련번호
var pSepcode ="";		//소송종결구분코드
var chnScrn;			//화면구분값 변수

/*===============================================================================
 * 00. callback 관련 Script
 *=============================================================================*/
/********************************************************************************
 * Desc		: callbackt시 evnet 처리
 * Param	: 없음
 * Return	: Mask값 
 *******************************************************************************/
 //callback 함수 (소송 정보 검색)
function fnCallback1(strId, errcode, errmsg){
	//소송 조건검색 콜백
	if(strId == "getLawsuitJudg"){
		if(errcode < 0){			
			alert("데이터 불러오기에 실패하였습니다." + errmsg);
		}else {
			// 피고사항목록 조회
			var pladef_Sep_Code ='';
			// 당사원고일때 피고항목을 불러오도록 처리
			if(Combo3.Value=="001"){
				pladef_Sep_Code=Quote('002');
			}else if(Combo3.Value=="002"){
				pladef_Sep_Code=Quote('001');
			}
			
			if(combo3.Value =="002"){
				static_defendant.Visible=false;
				static_plaintiff.Visible=true;
				
				if(chnScrn == "1" || chnScrn == "2"){
					getPladeJudglist(lawsrcrlt, pladef_Sep_Code);
					
				}else {
					getPseqJudglist(lawsrcrlt, pladef_Sep_Code, pSeq);
				}
			 }else{
				static_defendant.Visible=true;
				static_plaintiff.Visible=false;
				
				if(chnScrn == "1" || chnScrn == "2"){
					getPladeJudglist(lawsrcrlt, pladef_Sep_Code);
					
				}else {
					getPseqJudglist(lawsrcrlt, pladef_Sep_Code, pSeq);
				}
			 }
			 
			 getlawinfo(lawsrcrlt);	// 현재 진행중인 업무 파악 
		}
	}
}

// 심급 - 원피고사항목록 조회
function fnCallback2(strId, errcode, errmsg){
	if(strId == "getPladeJudglist"){
		if(errcode < 0){			
			alert("데이터 불러오기에 실패하였습니다." + errmsg);
		}else {
			setInstanceLevel(lawsrcrlt);
		}
	}
}

// 조정안 & 재판외화해 원피고일련번호 당 조회
function fnCallback7(strId, errcode, errmsg){
	if(strId == "getPseqJudglist"){
		if(errcode < 0){			
			alert("데이터 불러오기에 실패하였습니다." + errmsg);
		}else {
			Combo4.Value = pSepcode;
			setInstanceLevel(lawsrcrlt);
		}
	}
}

// 해당심급차수만 출력하는 기능(상세)
function fnCallbackt4(strId, errcode, errmsg){	
	if(strId == "InstanceCntAll"){
		if(errcode < 0){			
			alert("데이터 불러오기에 실패하였습니다." + errmsg);
		}else {
			var instcnt = ds_instcnt.GetColumn(0,"instAdmCode");	//소송관리번호별 총 심급 수
			setinstancedl(instcnt);	//심급차수 상세분리
				
			// 결재라인 불러오기
			var passport = getBookNo();
			aprv_search(passport);
		}
	}
}

//결재라인 조회
function fnCallback(strId, errcode, errmsg){
	if(strId == "outList"){
		if(errcode < 0){			
			alert("데이터 불러오기에 실패하였습니다." + errmsg);
		}else {
			
			//결제라인box (ds_position_code) 생성
			var cnt = 0;
			var cc="y";
			var loginSabun = SubStr(gb_Param,"4","10");
			
			for( i=0 ; i<=ds_position.GetRowCount() ; i++){		
				ds_position_code.AddRow();
					if(ds_position.GetColumn(i,"aprvInfoCondCode")=="Y"){			//결재완료
						ds_position_code.SetColumn(cnt, "codeNo","Y");
						ds_position_code.SetColumn(cnt, "codeName","결제완료");
					}else if(ds_position.GetColumn(i,"aprvInfoCondCode")=="N"){		//결재대기
						ds_position_code.SetColumn(cnt, "codeNo","N");
						ds_position_code.SetColumn(cnt, "codeName","결제대기");
					}else if(ds_position.GetColumn(i,"aprvInfoCondCode")=="Z"){		//반려
						ds_position_code.SetColumn(cnt, "codeNo","Z");
						ds_position_code.SetColumn(cnt, "codeName","반려");			
						cc="n";
					}else if(ds_position.GetColumn(i,"aprvInfoCondCode")=="A"){		//전결
						ds_position_code.SetColumn(cnt, "codeNo","A");
						ds_position_code.SetColumn(cnt, "codeName","전결");			
					}else{
					}
				cnt++;
			}
			
			//반려시 상신자만 결재라인 초기화
			if(cc=="n" && ds_position.GetColumn(0, "aprvprgrInfoWritSabun")=loginSabun){
			
				ds_position.ClearData();
				ds_position_code.ClearData();
				return false;
			}
			
			aprv_lock();
		}
	}
	
	if(strId == "LawsuitStatus"){
		if(errcode < 0){			
			alert("데이터 불러오기에 실패하였습니다." + errmsg);
		}else {
			//소송접수
			
			if(ds_lawsuitstatus.GetColumn(0,"tabLawsuit") >= 1){
				Parent.Button2.BKColor = "aquamarine";
				Parent.Button2.font = "Default, 10, Bold";
				
			}else{
				Parent.Button2.BKColor="white";
				Parent.Button2.font="default, 9, Default";
			}
			
			//응소제소
			if(ds_lawsuitstatus.GetColumn(0,"tabAlseSue") >= 1){
				Parent.Button3.BKColor = "aquamarine";
				Parent.Button3.font = "Default, 10, Bold";
				
			}else{
				Parent.Button3.BKColor="white";
				Parent.Button3.font="default, 9, Default";
			}
			
			//서증관리
			if(ds_lawsuitstatus.GetColumn(0,"tabEdocAdm") >= 1){
				Parent.Button4.BKColor = "aquamarine";
				Parent.Button4.font = "Default, 10, Bold";
				
			}else{
				Parent.Button4.BKColor="white";
				Parent.Button4.font="default, 9, Default";
			}
			
			//기일정보관리
			if(ds_lawsuitstatus.GetColumn(0,"tabLawFixinfo") >= 1){
				Parent.Button5.BKColor = "aquamarine";
				Parent.Button5.font = "Default, 10, Bold";
				
			}else{
				Parent.Button5.BKColor="white";
				Parent.Button5.font="default, 9, Default";
			}
			
			//준비서면
			if(ds_lawsuitstatus.GetColumn(0,"tabPreIwr") >= 1){
				Parent.Button6.BKColor = "aquamarine";
				Parent.Button6.font = "Default, 10, Bold";
				
			}else{
				Parent.Button6.BKColor="white";
				Parent.Button6.font="default, 9, Default";
			}
			
			//조정안
			if(ds_lawsuitstatus.GetColumn(0,"tabMediation") >= 1){
				Parent.Button7.BKColor = "aquamarine";
				Parent.Button7.font = "Default, 10, Bold";
				
			}else{
				Parent.Button7.BKColor="white";
				Parent.Button7.font="default, 9, Default";
			}
			
			
			//재판외화해
			if(ds_lawsuitstatus.GetColumn(0,"tabReconcil") >= 1){
				Parent.Button8.BKColor = "aquamarine";
				Parent.Button8.font = "Default, 10, Bold";
				
			}else{
				Parent.Button8.BKColor="white";
				Parent.Button8.font="default, 9, Default";
			}
			
			//판결보고
			if(ds_lawsuitstatus.GetColumn(0,"tabJudg") >= 1){
				Parent.Button9.BKColor = "aquamarine";
				Parent.Button9.font = "Default, 10, Bold";
				
			}else{
				Parent.Button9.BKColor="white";
				Parent.Button9.font="default, 9, Default";
			}
			
			//소송종결
			if(ds_lawsuitstatus.GetColumn(0,"tabPladef") >= 1){
				Parent.Button10.BKColor = "aquamarine";
				Parent.Button10.font = "Default, 10, Bold";
				
			}else{
				Parent.Button10.BKColor="white";
				Parent.Button10.font="default, 9, Default";
			}
		}
	}
}

// (원피고 정보 업데이트 콜백) -- 원피고종결사항
function updateCallbackP(strId, errcode, errmsg){	
	if(strId == "createPladefUp"){
		if(errcode < 0){			
			alert("데이터 불러오기에 실패하였습니다." + errmsg);
		}else {
			var cc=0;
			
			for(var i=0; i<ds_pladef.rowcount(); i++){	
				if(Length(ds_pladef.GetColumn(i, "pladefConcDate")) > 0
					&& Length(ds_pladef.GetColumn(i, "pladefLawConcSepcode")) > 0
					&& Length(ds_pladef.GetColumn(i, "pladefLawconcInstcode")) > 0
					&& Length(ds_pladef.GetColumn(i, "pladefConcCont")) > 0){
					
					cc++;
					
				}
			}
			
			if(cc==ds_pladef.rowcount){
				//소송종결일자 UPDATE
				Button1_OnClick();
				return;
			}
			alert("저장되었습니다.");
		}
	}
}     

