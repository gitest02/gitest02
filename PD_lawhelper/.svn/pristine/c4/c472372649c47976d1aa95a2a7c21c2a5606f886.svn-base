        	this.ds_preiwr.setColumn(0, "preIwrOutLyrName", lyrName);	
        	this.ds_preiwr.setColumn(0, "lawsuitAdmNo", lawsuitAdmNo);	
        	this.ds_preiwr.setColumn(0, "instAdmCode", instAdmCode);	
        }

