	today = yyyy+"-"+mm+"-"+dd;

	var nRow = this.ds_codelist.addRow();
	
	var nGroupVal;

	nGroupVal = this.ds_codemaster.getColumn(this.Grid00.getSelectedRows(),"class_code");
	
