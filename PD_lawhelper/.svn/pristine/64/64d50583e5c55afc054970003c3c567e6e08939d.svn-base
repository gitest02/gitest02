        //저장
        this.fn_save = function(strid)
        {
        	var lawId = this.div.edt_lawid.value;
        			if(lawId == null){
        				alert("법령 ID를 입력하세요");
        				return;
        			}
        			
        	var oDatas =  {
        			  svcid : strid,
