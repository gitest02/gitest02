// 결제대장번호 생성
function getBookNo(){

    var BookNo = "";	
    
    pSeq = ToString( ds_pladef.GetColumn( ds_pladef.row ,"pladefSeq" ) ); 
    var pTemp =""; 
    
    // 원피고일련번호
    if( pSeq != "" && Length( pSeq ) == 4 ){
    }else if(  pSeq != "" && Length( pSeq ) < 5 ){
       for(i=0; i<3; i++){
           pSeq = (pTemp+"0") + pSeq;  
       }
    }else{
        pSeq = "0000";
    }
        
	BookNo = ToString(lawsuit_no.Text)    		// 소송관리번호
	       + "007"                              // 업무구분코드
		   + ToString(Combo6.Value)  			// 심급차수	 
		   + pSeq;								// 원피고일련번호

   return BookNo;
}



