        		{
        		   //Iject.alert(this.errorMsg,"알림");	
        		   alert("실패하였습니다");
        		   return;
        		}			
        	      
        	       var pThis = this.fobj;   //form object
        	       
        	      switch(this.svcid)
        	      {
        	         case "save" : 
        				Iject.alert("저장완료");
        				break;
        	      } 
        	       var pThis = this.fobj;   //form object	      
        	       
        });
        }
        
        });


        
        // Regist UI Components Event
