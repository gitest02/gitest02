this.getBookNo = function()
{
	var bookNo = "";	
    
    var pSeq = toString( this.ds_mediation.getColumn( this.ds_mediation.row, "pladefSeq" ) );
    var mSeq = toString( this.ds_mediationDetail.getColumn( this.ds_mediationDetail.row, "mediationSeq" ) );       
        
	bookNo = toString( this.edt_law_adm_no.text )   // 소송관리번호
	       + "004"                             		// 업무구분코드
		   + toString( this.comb_inst_code.value )  // 심급차수	 
		   + pSeq                              		// 원피고일련번호
		   + mSeq;                            		// 조정안 일련번호 
  
   trace("bookNo 결재대장번호 ===> " + bookNo);
   
   return bookNo;
}


